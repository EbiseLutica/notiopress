{
  // サイトの設定リスト。設定することで、NotioPressインスタンスを複数建てること無く、複数のサイトを管理できます。
  sites: [
    {
      // ホスト名。ここで設定したホストに合致するリクエストに対し、本サイトをレスポンスとする。
      host: "example.com",
      // デフォルトであるかどうか。
      // trueであれば、リクエストがどのホストにも一致しない場合に本サイトをレスポンスとする。
      // default: true が複数あってはならない。エラーとなる。
      default: true,
      // カスタムCSSのパス。user-assetsディレクトリからの相対パス。
      customCss: "",
      // サイトのメタ情報
      meta: {
        // 本サイトの名前。
        title: "",
        // 本サイトのサブタイトル。
        subTitle: "",
      },
      // Notion設定
      notion: {
        token: "",
        databaseId: "",
      },
    }
  ]
}